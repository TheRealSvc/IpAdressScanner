{
  "name": "ipadressscanner",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": "6.14.12",
    "node": "14.16.1",
    "nodemon": "2.0.7"
  },
  "scripts": {
    "client": "cd frontend && serve -s build",
    "server": "cd server && node server.bundle.js",
    "dev": "concurrently --kill-others   \"cd ./server && npm start\" \"cd ./frontend/ && npm start\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nexe": "^4.0.0-beta.19",
    "nodemon": "^2.0.13",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
